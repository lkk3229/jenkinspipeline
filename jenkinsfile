    agent any
    stages {
  stage('build') {
    steps {
         echo 'Biding...'
    }
  }

  stage('test') {
    steps {
     echo 'Testing....'
    }
  }

  stage('deploy') {
    steps {
      echo 'Deploying...'
      echo 'Done.....!!!!'
    }
  }

}

}
